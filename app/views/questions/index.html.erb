<div class="container">
	<h1 class="title-lg mb">All questions</h1>

	<% @questions.each do |question| %>
		<p class="mt-sm"><%= link_to "Question #{question.id}", question_path(question) %></p>
	<% end %>

	<p class="mt">
		<%= link_to 'Create new question', new_question_path, class: 'button button-main' %>
	</p>
</div>
